<div metal:define-macro="portal_logo"
  id="portal-logo-bar">
        
    <a id="portal-logo-text"
      title="Home"
      accesskey="1"
      tal:attributes="href view/navigation_root_url;
      title view/navigation_root_title"
      i18n:domain="plone"
      i18n:attributes="title">
        <div id="portal-logo-left-text">
            <div class="portalLogoText">Case Western Reserve University</div>
        </div>
        <div id="portal-logo-right-text">
            <div class="portalLogoText">Engineers Without Borders</div>
        </div>
    </a>
    
    <a id="portal-logo-icon"
      title="Home"
      accesskey="1"
      tal:attributes="href view/navigation_root_url;
      title view/navigation_root_title"
      i18n:domain="plone"
      i18n:attributes="title">
        <object data="logo.svg" type="image/svg+xml" width="74" height="68">
            <img src="logo.png" alt=""
              tal:replace="structure view/logo_tag" />
        </object> 
    </a>
</div>